  <mapper namespace="com.dodam.mappers.MissingBoardMapper">
  
  <select id="selectAll" resultType="com.dodam.domain.missing.MissingBoardListDTO">
  	select no, title, writer, contact, age, breed, location, gender, img, thumbimg, missingdate, name from missingboard order by registerdate desc;
  </select>
  
